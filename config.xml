<?xml version='1.0' encoding='utf-8'?>
<widget id="com.finanzasPersonalaes.phonegap" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" versionCode="10">
    <name>Finanzas Personales</name>
    <description>
        Finanzas Personales es una aplicaci√≥n que te permite llevar el control de tus ingresos y egresos mensuales.
    </description>
    <author email="support@dhavico.com" href="http://dhavico.github.io">
        _dhavico Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-battery-status" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-camera" source="npm" spec="~2.1.1" />
    <plugin name="cordova-plugin-media-capture" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-contacts" source="npm" spec="~2.0.1" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-motion" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-device-orientation" source="npm" spec="~1.0.2" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-file" source="npm" spec="~4.1.1" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.5.0" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-globalization" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.2" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="android">
		<icon src="/img/logo.png" />
        <icon density="ldpi" src="/img/logo.png" gap:platform="android" />
        <icon density="mdpi" src="/img/logo.png" gap:platform="android" />
        <icon density="hdpi" src="/img/logo.png" gap:platform="android" />
        <icon density="xhdpi" src="/img/logo.png" gap:platform="android" />
        <icon density="xxhdpi" src="/img/logo.png" gap:platform="android" />
        <icon density="xxxhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-ldpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-mdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-hdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-xhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-xxhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="land-xxxhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-ldpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-mdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-hdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-xhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-xxhdpi" src="/img/logo.png" gap:platform="android" />
        <splash density="port-xxxhdpi" src="/img/logo.png" gap:platform="android" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
